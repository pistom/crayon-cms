<!doctype html>
<html class="no-js" lang="">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>{{ settings['site_name'] }} | Back Office</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="theme-color" content="#222f3c">
    <link rel="apple-touch-icon" href="{{ root }}/images/apple-touch-icon.png">
    <link rel="shortcut icon" href="{{ root }}/images/favicon.ico" type="image/x-icon" />
    <link rel="stylesheet" href="{{ root }}/_bo/css/5ec.min.css">
    <link href="https://fonts.googleapis.com/css?family=Lora|Muli:400,700&amp;subset=latin-ext" rel="stylesheet">
    <link rel="stylesheet" href="{{ settings['fontawesome_cdn'] ? settings['fontawesome_cdn'] : 'https://opensource.keycdn.com/fontawesome/4.7.0/font-awesome.min.css' }}">
    {% block style %}{% endblock %}
    {% block head %}{% endblock %}
</head>
<body>

<!--[if lt IE 8]>
<p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
<![endif]-->

<section class="content {{ contentClass }}">
    {% block content %}{% endblock %}
</section>

<div class="msgWindow"><span class="msgWindowClose"></span><div class="msgWindowTitle"></div><div class="msgWindowContent"></div></div>
<script src="{{ root }}/_bo/js/5ec.js"></script>
{% block iframeScripts %}{% endblock %}
{% block scripts %}
{% endblock %}

</body>
</html>